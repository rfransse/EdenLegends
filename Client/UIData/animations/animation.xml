			<KeyFrame position="0" value="set:CNDejaVu image:auto1" />
			<KeyFrame position="0.2" value="set:CNDejaVu image:auto2" />
			<KeyFrame position="0.4" value="set:CNDejaVu image:auto3" />
			<KeyFrame position="0.6" value="set:CNDejaVu image:auto4" />
			<KeyFrame position="0.8" value="set:CNDejaVu image:auto5" />
			<KeyFrame position="1" value="set:CNDejaVu image:auto6" />
		</Affector>
	</AnimationDefinition>
	
