	<AnimationDefinition name="InjuredFlash" duration="1.0" replayMode="loop" AutoStartAttribute="true">
		<Affector property="WindowAlpha"  interpolator="float">
			<KeyFrame position="0" value="0" />
			<KeyFrame position="0.5" value="1" />
			<KeyFrame position="1" value="0" />
		</Affector>
		
	</AnimationDefinition>
	
</Animations>

