<?xml version="1.0" ?>
<Animations>

    <AnimationDefinition name="Fadeout" duration="1.3" replayMode="once">
        <Affector property="WindowAlpha"  applicationMethod="relative" interpolator="float">
            <KeyFrame position="0" value="1" />
            <KeyFrame position="1.3" value="-1" progression="quadratic decelerating" />
        </Affector>
        <Subscription event="MouseLeavesArea" action="Start" />
    </AnimationDefinition>
	 <AnimationDefinition name="Fadein" duration="1.3" replayMode="once">
         <Affector property="WindowAlpha" applicationMethod="relative" interpolator="float">
            <KeyFrame position="0.0" value="-1" />
            <KeyFrame position="1.3" value="1" />
        </Affector>
        <Subscription event="MouseEntersArea" action="Start" />
    </AnimationDefinition>
    <AnimationDefinition name="ShakeX" duration="0.07" replayMode="once">
        <Affector property="UnifiedXPosition" applicationMethod="relative" interpolator="UDim">
            <KeyFrame position="0.00" value="{0.0,5}" />
            <KeyFrame position="0.01" value="{0.0,0.0}" />
			<KeyFrame position="0.02" value="{0.0,-5.0}"  />	
			<KeyFrame position="0.03" value="{0.0,0.0}"  />	
			<KeyFrame position="0.04" value="{0.0,5}" />
            <KeyFrame position="0.05" value="{0.0,0.0}" />
			<KeyFrame position="0.06" value="{0.0,-5.0}"  />	
			<KeyFrame position="0.07" value="{0.0,0.0}"  />	
        </Affector>
		 <Subscription event="Shown" action="Start" />
        
    </AnimationDefinition>
	<AnimationDefinition name="Move" duration="1.0" replayMode="once">
        <Affector property="UnifiedXPosition" interpolator="UDim">
			<KeyFrame position="0.0" value="{0.0,0.0}"  />	
			<KeyFrame position="1.0" value="{-1.0,0.0}"  />	
        </Affector>
        
    </AnimationDefinition>
	<AnimationDefinition name="Moveto" duration="1.0" replayMode="once">
        <Affector property="UnifiedXPosition" interpolator="UDim">
			<KeyFrame position="0.0" value="{-1.0,0.0}"  />	
			<KeyFrame position="1.0" value="{0.0,0.0}"  />	
        </Affector>
        
    </AnimationDefinition>
	<AnimationDefinition name="Resize" duration="0.25" replayMode="once">
		<Affector property="WindowAlpha" interpolator="float">
            <KeyFrame position="0" value="1.0"  />
            <KeyFrame position="0.23" value="0.1"   />
			<KeyFrame position="0.25" value="1.0"progression="discrete"/>
        </Affector>
        <Affector property="UnifiedSize" interpolator="UVector2">
            <KeyFrame position="0" value="{{0,30},{0,30}}" />
            <KeyFrame position="0.23" value="{{0,80},{0,80}}"  />
			<KeyFrame position="0.25" value="{{0,30},{0,30}}"  progression="discrete" />
        </Affector>
        <Subscription event="MouseButtonDown" action="Start" />
    </AnimationDefinition>
	<AnimationDefinition name="Resize2" duration="0.25" replayMode="once" AutoStartAttribute="false">
		<Affector property="WindowAlpha" applicationMethod="relative" interpolator="float">
            <KeyFrame position="0" value="1.0" />
			<KeyFrame position="0.25" value="0.1"   />
		</Affector>
        <Affector property="UnifiedSize" applicationMethod="relative" interpolator="UVector2">
            <KeyFrame position="0" value="{{0,0},{0,0}}"  /> 
		    <KeyFrame position="0.24" value="{{0,10},{0,10}}"  />
			<KeyFrame position="0.25" value="{{0,0},{0,0}}"  />
        </Affector>
        <Subscription event="MouseButtonDown" action="Start" />
    </AnimationDefinition>
	
	<AnimationDefinition name="AchievementTip" duration="3.5" replayMode="once" AutoStartAttribute="false">
	    <Affector property="WindowAlpha" applicationMethod="relative" interpolator="float">
            <KeyFrame position="0" value="0" />
			<KeyFrame position="0.5" value="-1"progression="quadratic accelerating" />
			<KeyFrame position="1" value="1"  progression="quadratic accelerating" />
			<KeyFrame position="1.5" value="-1"  progression="quadratic accelerating" />
			<KeyFrame position="2" value="1"   progression="quadratic accelerating" />
			<KeyFrame position="2.5" value="-1"    progression="quadratic accelerating" />
			<KeyFrame position="3" value="1"   progression="quadratic accelerating" />
			<KeyFrame position="3.5" value="-1" progression="quadratic accelerating" />
			
			
		</Affector>
		
        <Subscription event="Shown" action="Start" />
    </AnimationDefinition>
	<AnimationDefinition name="AchievementTipHide" duration="0.5" replayMode="once" AutoStartAttribute="false">
	    <Affector property="WindowAlpha" applicationMethod="relative" interpolator="float">
            <KeyFrame position="0" value="0" />
			<KeyFrame position="0.5" value="-1" />
		</Affector>
		
    </AnimationDefinition>
	
	<AnimationDefinition name="RollWndAnimation" duration="20.0" replayMode="once" AutoStartAttribute="false">
	    <Affector property="CurrentProgress"  interpolator="float">
            <KeyFrame position="0.0" value="1" />
			<KeyFrame position="20.0" value="0" />
		</Affector>
		
    </AnimationDefinition>

	<AnimationDefinition name="BuffAnimation" duration="5.0" replayMode="once" AutoStartAttribute="false">
		<Affector property="WindowAlpha" interpolator="float">
			<KeyFrame position="0" value="1" />
			<KeyFrame position="0.5" value="0.1" />
			<KeyFrame position="1" value="1" />
			<KeyFrame position="1.5" value="0.1" />
			<KeyFrame position="2" value="1" />
			<KeyFrame position="2.5" value="0.1" />
			<KeyFrame position="3" value="1" />
			<KeyFrame position="3.5" value="0.1" />
			<KeyFrame position="4" value="1" />
			<KeyFrame position="4.5" value="0.1" />
			<KeyFrame position="5" value="1" />
		</Affector>
	</AnimationDefinition>
	
	<AnimationDefinition name="NewPlayerGuideAnimation" duration="1" replayMode="Loop" AutoStartAttribute="false">
